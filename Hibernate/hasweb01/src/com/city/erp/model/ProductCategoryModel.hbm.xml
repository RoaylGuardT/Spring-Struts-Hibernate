<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.city.erp.model">

	<class name="ProductCategoryModel" table="ERP_ProductCategory">
		<id name="pcno" column="PCNO">
			<generator class="native" />
		</id>
		<property name="pccode" column="PCCODE" />
		<property name="pcname" column="PCNAME" />
	</class>

</hibernate-mapping>
