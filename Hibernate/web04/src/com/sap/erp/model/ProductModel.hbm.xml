<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sap.erp.model">

	<class name="ProductModel" table="erp_product">
		<id name="pno" column="PNO">
			<generator class="native" />
		</id>
		<property name="pname" column="PNAME" />
		<property name="pdate" column="PDATE" />
		<property name="uniprice" column="UNITPRICE" />
		<property name="qty" column="QTY" />
		
	</class>

</hibernate-mapping>
